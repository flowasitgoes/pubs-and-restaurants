{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/buri/(.*)",
      "dest": "/public/buri-master/$1"
    },
    {
      "src": "/flatter/(.*)",
      "dest": "/public/flatter-master/$1"
    },
    {
      "src": "/foody/(.*)",
      "dest": "/public/foody-master/$1"
    },
    {
      "src": "/kusina/(.*)",
      "dest": "/public/kusina-master/$1"
    },
    {
      "src": "/tasteit/(.*)",
      "dest": "/public/tasteit-master/$1"
    },
    {
      "src": "/buri",
      "dest": "/public/buri-master/index.html"
    },
    {
      "src": "/flatter",
      "dest": "/public/flatter-master/index.html"
    },
    {
      "src": "/foody",
      "dest": "/public/foody-master/index.html"
    },
    {
      "src": "/kusina",
      "dest": "/public/kusina-master/index.html"
    },
    {
      "src": "/tasteit",
      "dest": "/public/tasteit-master/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 